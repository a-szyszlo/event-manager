{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/wp-admin/",
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "steps": [
    {
      "step": "login",
      "username": "admin"
    },
    {
      "step": "runCommand",
      "command": "curl -L https://github.com/a-szyszlo/event-manager/archive/refs/heads/main.zip -o /tmp/event-manager.zip"
    },
    {
      "step": "installPlugin",
      "pluginFilePath": "/tmp/event-manager.zip",
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginSlug": "advanced-custom-fields",
      "options": {
        "activate": true
      }
    }
  ]
}