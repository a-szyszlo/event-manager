{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/wp-admin/",
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "steps": [
    {
      "step": "login",
      "username": "admin"
    },

    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "url",
        "url": "https://codeload.github.com/a-szyszlo/event-manager/zip/refs/heads/main"
      },
      "options": {
        "activate": true
      }
    },

    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "advanced-custom-fields"
      },
      "options": {
        "activate": true
      }
    }
  ]
}